{
	"manifest_version" : 3,
	"name" : "CopyTabs",
	"version" : "0.002",
    "background":{
        "service_worker":"background.js",
        "type":"module"
    },
    "permissions":[
        "downloads",
        "tabs"
    ]
    ,
    "action" : {
        "default_icon":"icon.png",
        "default_popup": "popup.html"

    }
    

    ,
    "icons":{
        "16":"icon.png",
        "48":"icon.png",
        "128":"icon.png"
    },
    "description": "Export and import tabs in a snap!"
}